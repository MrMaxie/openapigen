{
    "name": "oapicodegen",
    "private": false,
    "version": "1.0.1",
    "description": "OpenAPI 2/3 transformer & codegen",
    "keywords": [
        "openapi",
        "typescript",
        "codegen",
        "code-generation",
        "api",
        "swagger",
        "type-safety",
        "rest-api",
        "codegen",
        "converter",
        "json-schema",
        "client-library",
        "types"
    ],
    "type": "module",
    "main": "./dist/main.cjs.js",
    "module": "./dist/main.es.js",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "import": "./dist/main.es.js",
            "require": "./dist/main.cjs.js"
        }
    },
    "scripts": {
        "build": "tsc && vite build"
    },
    "devDependencies": {
        "@types/axios": "0.14.0",
        "@types/lodash": "4.14.192",
        "@types/node": "18.15.10",
        "@types/prettier": "^2.7.2",
        "@types/swagger2openapi": "7.0.0",
        "@zodios/core": "10.8.1",
        "axios": "1.3.4",
        "openapi-zod-client": "1.6.1",
        "prettier": "^2.8.7",
        "swagger-typescript-api": "12.0.4",
        "typescript": "5.0.2",
        "vite": "4.2.1",
        "vite-plugin-dts": "2.1.0"
    },
    "dependencies": {
        "swagger2openapi": "7.0.8",
        "zod": "3.21.4"
    }
}
